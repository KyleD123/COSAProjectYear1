      <ImageView id="btnEditTeam" fx:id="btnEdit" fitHeight="150.0" fitWidth="200.0" layoutX="618.0" layoutY="300.0" onMouseClicked="#switchModifyScene" pickOnBounds="true" preserveRatio="true">
